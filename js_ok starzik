ctx.fillStyle = '#6a4a2a';
ctx.fillRect(x - 13, y + 87, 18, 3);
ctx.fillRect(x - 13, y + 105, 18, 3);

// Uchwyt wiadra
ctx.strokeStyle = '#6a4a2a';
ctx.lineWidth = 2;
ctx.beginPath();
ctx.moveTo(x - 8, y + 87);
ctx.lineTo(x - 5, y + 82);
ctx.lineTo(x + 5, y + 82);
ctx.lineTo(x + 8, y + 87);
ctx.stroke();

// Woda w wiadrze
ctx.fillStyle = '#4a6a8a';
ctx.fillRect(x - 10, y + 89, 12, 14);

// Mop w rƒôku - lepszy
ctx.fillStyle = '#8a6a4a';
ctx.fillRect(x + 50, y + 30, 3, 40);

// G≈Ç√≥wka mopa
ctx.fillStyle = '#6a6a5a';
ctx.fillRect(x + 48, y + 26, 7, 8);

// Sznurki mopa
ctx.strokeStyle = '#5a5a4a';
ctx.lineWidth = 1;
for(let i = 0; i < 5; i++) {
ctx.beginPath();
ctx.moveTo(x + 49 + i, y + 34);
ctx.lineTo(x + 48 + i, y + 40 + Math.random() * 3);
ctx.stroke();
}
}

