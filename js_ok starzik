// W≈Çosy pod chustkƒÖ (siwe)
ctx.fillStyle = '#4a4a4a';
ctx.fillRect(x + 19, y + 21, 20, 8);

// Chustka na g≈Çowie (w kropki)
ctx.fillStyle = '#6a5a5a';
